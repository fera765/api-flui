{
  "name": "api-backend",
  "version": "1.0.0",
  "description": "API Backend with Clean Architecture, DDD and SOLID principles",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/tests/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=src/tests/integration --passWithNoTests",
    "test:e2e": "NODE_ENV=test jest tests/e2e --runInBand --detectOpenHandles --passWithNoTests",
    "test:e2e:watch": "NODE_ENV=test jest tests/e2e --runInBand --detectOpenHandles --watch",
    "test:e2e:verbose": "NODE_ENV=test jest tests/e2e --runInBand --detectOpenHandles --verbose",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "clean-architecture",
    "ddd",
    "solid"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/semver": "^7.7.1",
    "@types/supertest": "^6.0.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "multer": "^2.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@types/axios": "^0.9.36",
    "cors": "^2.8.5",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.4"
  }
}
